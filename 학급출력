import java.util. *;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        System.out.print("입력 : ");
        int a = scan.nextInt();
        switch(a){
            case 1:
            case 2:
            case 3:
            case 4: System.out.print("뉴미디어소프트웨어과");break;
            case 5:
            case 6: System.out.print("뉴미디어디자인과");break;
            default: break;
        }
    }
}
